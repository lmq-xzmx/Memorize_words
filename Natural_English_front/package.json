{
  "name": "natural-english-front",
  "version": "1.0.0",
  "description": "Natural English Learning H5 App",
  "main": "main.ts",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "clear-cache": "tsx clear-cache.ts",
    "dev-fresh": "tsx clear-cache.ts && vite",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "echarts": "^6.0.0",
    "element-plus": "^2.10.7",
    "vue": "^3.3.0",
    "vue-router": "^4.2.0",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "@types/node": "^20.19.11",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vue/test-utils": "^2.4.0",
    "@vue/vue3-jest": "^29.0.0",
    "autoprefixer": "^10.4.14",
    "babel-jest": "^29.0.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "postcss": "^8.4.24",
    "sass": "^1.63.6",
    "terser": "^5.19.0",
    "tsx": "^4.7.0",
    "typescript": "^5.0.0",
    "vite": "^4.0.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-pwa": "^0.16.4",
    "vue-tsc": "^1.8.0",
    "workbox-window": "^7.0.0"
  },
  "browserslist": [
    "Android >= 4.4",
    "ios >= 9"
  ]
}
